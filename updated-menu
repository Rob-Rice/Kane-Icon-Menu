<script>
const weather = document.querySelector('.weather')
window.addEventListener('load', function() {
fetch("https://api.openweathermap.org/data/2.5/weather?lat=41.663071&lon=-78.811089&units=imperial&APPID=2e9cf6d87e2ddd9c1b51c1d463ecc780")    .then(response => response.json())
.then(data => {
let tempValue = data['main']['temp'].toFixed();
weather.innerHTML = tempValue;
})
})
</script>
